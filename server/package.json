{
   "name": "@streamer/server",
   "version": "1.0.0",
   "main": "index.ts",
   "license": "MIT",
   "scripts": {
      "test": "jest --runInBand",
      "launch": "cross-env TS_NODE_PROJECT=./tsconfig.app.json node -r ts-node/register ./index.ts",
      "start": "node ./dist/server/index.js",
      "build": "docker build -t server .",
      "precompile": "rm -rf dist",
      "compile": "tsc -p ./tsconfig.app.json",
      "postcompile": "cp ormconfig.json ../dist/server/ormconfig.json",
      "orm": "cross-env APP_ROOT_PATH='.' TS_NODE_PROJECT=./tsconfig.app.json node -r ts-node/register ../node_modules/typeorm/cli.js"
   },
   "dependencies": {
      "@types/bcrypt": "^3.0.0",
      "@types/chai": "^4.1.7",
      "@types/express": "^4.16.1",
      "@types/jest": "^24.0.12",
      "@types/jsonwebtoken": "^8.3.8",
      "apollo-server": "^2.3.1",
      "apollo-server-express": "^2.5.0",
      "axios": "^0.18.1",
      "bcrypt": "^3.0.3",
      "chai": "^4.2.0",
      "class-transformer": "^0.2.0",
      "concurrently": "^5.1.0",
      "cross-env": "^7.0.0",
      "express": "^4.17.0",
      "express-history-api-fallback": "^2.2.1",
      "graphql": "^14.0.2",
      "jest": "^25.1.0",
      "jsonwebtoken": "^8.4.0",
      "mysql": "^2.16.0",
      "reflect-metadata": "^0.1.10",
      "source-map-url": "^0.4.0",
      "sqlite3": "^4.0.7",
      "ts-jest": "^24.0.2",
      "tslib": "^1.11.1",
      "type-graphql": "^0.16.0",
      "typedi": "^0.8.0",
      "typeorm": "0.2.11",
      "typeorm-typedi-extensions": "^0.2.1",
      "typescript": "^3.4.5",
      "winston": "^3.2.1"
   },
   "devDependencies": {
      "@types/node": "^8.0.29",
      "nodemon": "^1.18.9",
      "ts-node": "8.7.0"
   }
}
